name: PicsArt
desc: 解锁高级功能

http:
  mitm:
    - "api.picsart.c*"
    - "api.meiease.c*"
  script:
    - match: https:\/\/api\.(picsart|meiease)\.c(n|om)\/users\/show\/me\.json
      name: PicsArt
      type: response
      require-body: true
      timeout: 10
      
script-providers:
  PicsArt:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/PicsArt.js
    interval: 3600
