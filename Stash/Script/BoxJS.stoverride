name: BoxJS
desc: 安装后访问http://boxjs.com/进入

http:
  mitm:
    - "boxjs.com"
  script:
    - match: ^https?://boxjs.com
      name: boxjs
      type: request
      require-body: true
      timeout: 10
      
script-providers:
  boxjs:
    url: https://raw.githubusercontent.com/chavyleung/scripts/master/chavy.box.js
    interval: 3600
